i3conf: {
  modifier = "Mod4";
  terminal = "kitty";
  keybindings = {
    "${i3conf.modifier}+Return" = "exec ${i3conf.terminal}";
    "${i3conf.modifier}+slash" = "exec ${i3conf.menu}";
    "${i3conf.modifier}+h" = "focus left";
    "${i3conf.modifier}+n" = "focus down";
    "${i3conf.modifier}+e" = "focus up";
    "${i3conf.modifier}+i" = "focus right";
    "${i3conf.modifier}+Shift+h" = "move left";
    "${i3conf.modifier}+Shift+n" = "move down";
    "${i3conf.modifier}+Shift+e" = "move up";
    "${i3conf.modifier}+Shift+i" = "move right";
    "${i3conf.modifier}+b" = "split h";
    "${i3conf.modifier}+v" = "split v";
    "${i3conf.modifier}+f" = "fullscreen toggle";
    "${i3conf.modifier}+s" = "layout stacking";
    "${i3conf.modifier}+w" = "layout tabbed";
    "${i3conf.modifier}+g" = "layout toggle split";
    "${i3conf.modifier}+Shift+numbersign" = "floating toggle";
    "${i3conf.modifier}+numbersign" = "focus mode_toggle";
    "${i3conf.modifier}+a" = "focus parent";
    "${i3conf.modifier}+Shift+minus" = "move scratchpad";
    "${i3conf.modifier}+minus" = "scratchpad show";
    "${i3conf.modifier}+1" = "workspace number 1";
    "${i3conf.modifier}+2" = "workspace number 2";
    "${i3conf.modifier}+3" = "workspace number 3";
    "${i3conf.modifier}+4" = "workspace number 4";
    "${i3conf.modifier}+5" = "workspace number 5";
    "${i3conf.modifier}+6" = "workspace number 6";
    "${i3conf.modifier}+7" = "workspace number 7";
    "${i3conf.modifier}+8" = "workspace number 8";
    "${i3conf.modifier}+9" = "workspace number 9";
    "${i3conf.modifier}+0" = "workspace number 10";
    "${i3conf.modifier}+Shift+1" = "move container to workspace number 1";
    "${i3conf.modifier}+Shift+2" = "move container to workspace number 2";
    "${i3conf.modifier}+Shift+3" = "move container to workspace number 3";
    "${i3conf.modifier}+Shift+4" = "move container to workspace number 4";
    "${i3conf.modifier}+Shift+5" = "move container to workspace number 5";
    "${i3conf.modifier}+Shift+6" = "move container to workspace number 6";
    "${i3conf.modifier}+Shift+7" = "move container to workspace number 7";
    "${i3conf.modifier}+Shift+8" = "move container to workspace number 8";
    "${i3conf.modifier}+Shift+9" = "move container to workspace number 9";
    "${i3conf.modifier}+Shift+0" = "move container to workspace number 10";
    "${i3conf.modifier}+Shift+c" = "reload";
    "${i3conf.modifier}+Shift+r" = "restart";
    "${i3conf.modifier}+Shift+x" = "exec i3-nagbar -t warning -m 'Do you want to exit i3?' -b 'Yes' 'i3-msg exit'";
    "${i3conf.modifier}+r" = "mode resize";
  };
}
